{
  "name": "derrickyew.github.io",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:css": "postcss src/styles/index.css --use postcss-import --use autoprefixer --use cssnano --no-map -o src/styles/output.css",
    "watch:css": "postcss src/styles/index.css --use postcss-import --use autoprefixer --use cssnano --no-map -o src/styles/output.css -w --verbose",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@material-tailwind/react": "^2.1.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0"
  },
  "devDependencies": {
    "@types/react": "18.2.19",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.4.41",
    "postcss-cli": "^11.0.0",
    "postcss-conditionals": "^2.1.0",
    "postcss-css-variables": "^0.19.0",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^6.2.0",
    "prettier": "3.3.3",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  }
}
